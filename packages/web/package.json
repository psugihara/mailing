{
  "name": "web",
  "version": "0.4.14",
  "private": true,
  "scripts": {
    "dev": "yarn prisma migrate dev && next dev",
    "ci:server": "yarn prisma generate && yarn prisma migrate deploy && next dev",
    "build": "yarn prisma generate && yarn prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc",
    "build-emails": "cp -r ./emails/assets public && yarn mailing server build",
    "dev-emails": "yarn mailing",
    "dev-emails:local-assets": "npx mailing server scaffold && ln -sf ../../emails/assets ./.mailing/public && LOCAL_ASSETS=true yarn mailing preview"
  },
  "dependencies": {
    "@docsearch/react": "^3.3.0",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "2.1.2",
    "@next/font": "^13.0.5",
    "@next/mdx": "^13.0.5",
    "@prisma/client": "^4.2.0",
    "@vercel/analytics": "^0.1.5",
    "classnames": "^2.3.2",
    "mailing-core": "^0.9.5",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.4.0",
    "rehype-highlight": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.28.0",
    "eslint-config-next": "13.0.5",
    "jest-mock-extended": "^2.0.7",
    "mailing": "^0.9.5",
    "postcss": "^8.4.14",
    "prisma": "^4.2.0",
    "tailwindcss": "^3.2.4"
  }
}
